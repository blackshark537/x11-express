{
  "name": "x11-Express",
  "version": "Beta 0.1.0",
  "description": "Simple flow dynamic backend framework.",
  "main": "app.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "start": "tsc && node --unhandled-rejections=strict ./dist/main.js",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js -e .ts,.json  --exec \"npm run start\"",
    "debug": "export DEBUG=* && npm run start",
    "test": "nodemon ./dist/main.js"
  },
  "keywords": [
    "x11-Express",
    "Express X11",
    "drawflow"
  ],
  "author": "Blackshark537",
  "license": "MIT",
  "dependencies": {
    "@vanillawc/wc-monaco-editor": "^1.10.15",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "mongoose": "^6.3.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/socket.io-parser": "^3.0.0",
    "nodemon": "^2.0.16",
    "source-map-support": "^0.5.21",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  }
}
